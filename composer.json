{
  "name": "dmhendricks/wordpress-base-plugin",
  "type": "wordpress-plugin",
  "description": "A bloilerplate for WordPress plugins",
  "keywords": ["wordpress", "plugin", "boilerplate", "carbonfields"],
  "homepage": "https://2lab.net",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Daniel M. Hendricks",
      "homepage": "https://danhendricks.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/dmhendricks/wordpress-base-plugin/issues/",
    "wiki": "https://github.com/dmhendricks/wordpress-base-plugin/wiki/",
    "source": "https://github.com/dmhendricks/wordpress-base-plugin/"
  },
  "repositories": [
      {
          "type": "package",
          "package": {
            "name": "jashkenas/underscore",
            "version": "1.8.3",
            "dist": {
              "type": "zip",
              "url": "https://github.com/jashkenas/underscore/archive/1.8.3.zip",
              "reference": "master"
            },
            "autoload": {
                "classmap": ["."]
            }
        }
      }
  ],
  "require": {
    "php": ">=5.3.29",
    "mnsami/composer-custom-directory-installer": "^1.1.0",
    "htmlburger/carbon-fields": "^2.0.0",
    "jjgrainger/posttypes": "dev-master",
    "wpupdatephp/wp-update-php": "^1.1",
    "jashkenas/underscore": ">=1.8.0",
    "composer/installers": "~1.3.0"
  },
  "extra":{
    "installer-paths":{
      "./vendor/htmlburger/carbon-fields": ["htmlburger/carbon-fields"]
    }
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Nimbium\\MyPlugin\\": "app/"
    },
    "files": ["vendor/wpupdatephp/wp-update-php/src/WPUpdatePhp.php"]
  },
  "scripts": {
      "pre-autoload-dump": [
        "find ./vendor -name '.git*' -exec rm -rf {} +"
      ]
  }
}
